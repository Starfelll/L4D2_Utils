        length 10 \\
        tip_mass 0 \\
        pitch_friction 0 \\
        yaw_friction 0 \\
        pitch_stiffness 70 \\
        yaw_stiffness 70 \\
        pitch_damping 10 \\
        yaw_damping 10 \\
        //pitch_constraint $p_min$ $p_max$ \\
        //yaw_constraint $y_min$ $y_max$ \\
        angle_constraint $ang$ \\
    } } \\
